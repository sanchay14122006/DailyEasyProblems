class Solution {
    public int lengthOfLastWord(String s) {
        String s1 = s.trim();
        System.out.println(s1);
        int l = s1.length();
        char ch;
        int len = 0;
        for(int i=l-1;i>=0;i--)
        {
            ch = s1.charAt(i);
            if(ch==' ')
                break;
            len++;
        }
        return len;
    }
}
